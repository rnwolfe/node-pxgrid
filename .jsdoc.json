{
  "tags": {
    "allowUnknownTags": false
  },
  "templates": {
    "default": {
      "staticFiles": {
        "include": []
      }
    }
  },
  "source": {
    "include": [
      "./README.md",
      "./lib"
    ],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "template": "./node_modules/docdash",
    "encoding": "utf8",
    "destination": "docs/",
    "recurse": true,
    "verbose": true
  }
}